{
  "name":    "onyxpoint-compliance_markup",
  "version": "0.1.0",
  "author":  "Trevor Vaughan",
  "summary": "Additions to enable compliance annotations in Puppet code",
  "license": "Apache 2.0",
  "source":  "https://github.com/onyxpoint/compliance_markup",
  "project_page": "https://github.com/onyxpoint/compliance_markup",
